<odoo>
  <data>
     <record model="ir.ui.view" id="fastsnail.competition_form_view">
            <field name="name">Competitions</field>
            <field name="model">fastsnail.competition</field>
            <field name="arch" type="xml">
              <form>
                <sheet>
                      <group>
                        <field name="name"/>
                        <field name="idcompetition"/>
                        <field name="startdate"/>
                        <field name="enddate"/>
                        <field name="totalSeasons"/>                     
                      </group>
                  </sheet>
                </form>
              </field>
            </record>

            <record model="ir.ui.view" id="fastsnail.competition_tree_view">
            <field name="name">Competitions</field>
            <field name="model">fastsnail.competition</field>
            <field name="arch" type="xml">
              <tree>
                        <field name="name"/>
                        <field name="idcompetition"/>
                        <field name="startdate"/>
                        <field name="enddate"/>
                        <field name="totalSeasons"/>     
                </tree>
              </field>
            </record>

  </data>
</odoo>